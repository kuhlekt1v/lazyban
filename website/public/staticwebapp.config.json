{
	"navigationFallback": {
		"rewrite": "/index.html"
	},
	"routes": [
		{
			"route": "/*",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			}
		},
		{
			"route": "/index.html",
			"headers": {
				"cache-control": "public, max-age=3600, must-revalidate"
			}
		}
	],
	"mimeTypes": {
		".svg": "image/svg+xml"
	},
	"responseOverrides": {
		"404": {
			"rewrite": "/index.html"
		}
	}
}
